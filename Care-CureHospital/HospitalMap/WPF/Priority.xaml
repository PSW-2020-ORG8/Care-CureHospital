<Window x:Class="HospitalMap.WPF.Priority"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HospitalMap.WPF"
        mc:Ignorable="d"
        Title="Priority" Height="450" Width="800" WindowStartupLocation="CenterScreen" Background="Gray">
    <Grid>
        <Grid>
            <TextBlock HorizontalAlignment="Left" Height="38" Margin="100,166,0,0" Text="Select priority:" TextWrapping="Wrap" VerticalAlignment="Top" Width="244" FontSize="24"/>
            <ComboBox x:Name="Prioritet" HorizontalAlignment="Left" Height="38" Margin="400,166,0,0" VerticalAlignment="Top" Width="296" >
                <ComboBoxItem Content="Doctor"/>
                <ComboBoxItem Content="Time period"/>
            </ComboBox>
            <Button Background="CornflowerBlue" Content="Next" HorizontalAlignment="Left" Margin="617,358,0,0" VerticalAlignment="Top" Height="33" Width="110" FontSize="24" Click="Button_Click"/>
            <Button Background="CornflowerBlue" Content="Back" HorizontalAlignment="Left" Margin="462,358,0,0" VerticalAlignment="Top" Height="33" Width="110" FontSize="24" Click="Button_Click_1"/>

        </Grid>

    </Grid>
</Window>
